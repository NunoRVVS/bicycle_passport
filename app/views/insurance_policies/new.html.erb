<h1>New Insurance Policy</h1>
<%= form_with model: @Insurance_policy, local: true do |form| %>
  <div>
    <%= form.label :insurer %>
    <%= form.text_field :insurer %>
  </div>
  <div>
    <%= form.label :policy_number %>
    <%= form.text_field :policy_number %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to 'Back', components_path %>
