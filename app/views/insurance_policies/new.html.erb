<h1>New Insurance Policy</h1>
<%= form_with model: [@bicycle, @insurance_policy], local: true do |form| %>
  <div>
    <%= form.label :insurer %>
    <%= form.text_field :insurer %>
  </div>
  <div>
    <%= form.label :policy_number %>
    <%= form.text_field :policy_number %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to 'Back', bicycle_insurance_policies_path %>
