<h1>New Warranty</h1>
<%= form_with model: [@bicycle, @warranty], local: true do |form| %>
   <div>
    <%= form.label :issuer %>
    <%= form.text_field :issuer %>
  </div>
  <div>
    <%= form.label :start_date %>
    <%= form.text_field :start_date %>
  </div>
  <div>
    <%= form.label :end_date %>
    <%= form.text_field :end_date %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to 'Back', bicycle_warranties_path %>
